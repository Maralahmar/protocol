{
  "name": "eventory",
  "private": true,
  "version": "1.0.0",
  "workspaces": ["client", "server"],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "build": "npm run build -w eventory-client && npm run build -w eventory-server"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
